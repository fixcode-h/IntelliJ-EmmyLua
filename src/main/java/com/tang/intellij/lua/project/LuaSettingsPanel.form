<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.tang.intellij.lua.project.LuaSettingsPanel">
  <scrollpane id="scrollPane" binding="myPanel">
    <constraints>
      <xy x="0" y="0" width="600" height="500"/>
    </constraints>
    <properties>
      <horizontalScrollBarPolicy value="31"/>
      <verticalScrollBarPolicy value="22"/>
    </properties>
    <border type="none"/>
    <children>
      <tabbedpane id="mainTabbedPane" binding="contentPanel">
        <constraints/>
        <properties>
          <tabPlacement value="1"/>
        </properties>
        <border type="none"/>
        <children>
          <!-- 基本设置标签页 -->
          <grid id="basicSettingsTab" layout-manager="GridLayoutManager" row-count="6" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="10" vgap="10">
            <margin top="15" left="15" bottom="15" right="15"/>
            <constraints>
              <tabbedpane title="基本设置"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <!-- 构造函数名称 -->
              <component id="constructorNamesLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text resource-bundle="LuaBundle" key="ui.settings.constructor_names"/>
                </properties>
              </component>
              <component id="constructorNamesField" class="javax.swing.JTextField" binding="constructorNames">
                <constraints>
                  <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                    <preferred-size width="300" height="-1"/>
                  </grid>
                </constraints>
                <properties/>
              </component>
              
              <!-- Require函数名称 -->
              <component id="requireFunctionNamesLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text resource-bundle="LuaBundle" key="ui.settings.require_like_function_names"/>
                </properties>
              </component>
              <component id="requireFunctionNamesField" class="javax.swing.JTextField" binding="requireFunctionNames">
                <constraints>
                  <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                    <preferred-size width="300" height="-1"/>
                  </grid>
                </constraints>
                <properties/>
              </component>
              
              <!-- 语言级别 -->
              <component id="languageLevelLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="Language Level:"/>
                </properties>
              </component>
              <component id="languageLevelCombo" class="javax.swing.JComboBox" binding="languageLevel">
                <constraints>
                  <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="2" anchor="8" fill="1" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
              </component>
              
              <!-- 文件大小阈值 -->
              <component id="fileSizeThresholdLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="Too larger file threshold (KiB):"/>
                </properties>
              </component>
              <component id="fileSizeThresholdField" class="javax.swing.JTextField" binding="tooLargerFileThreshold">
                <constraints>
                  <grid row="3" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                    <preferred-size width="150" height="-1"/>
                  </grid>
                </constraints>
                <properties/>
              </component>
              
              <!-- 基本选项 -->
              <grid id="basicOptionsPanel" layout-manager="GridLayoutManager" row-count="4" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="5">
                <margin top="10" left="0" bottom="0" right="0"/>
                <constraints>
                  <grid row="4" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="etched" title="基本选项"/>
                <children>
                  <component id="strictDocCheckBox" class="javax.swing.JCheckBox" binding="strictDoc">
                    <constraints>
                      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="2" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <text resource-bundle="LuaBundle" key="ui.settings.strict_doc"/>
                    </properties>
                  </component>
                  <component id="smartCloseEndCheckBox" class="javax.swing.JCheckBox" binding="smartCloseEnd">
                    <constraints>
                      <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="2" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <text resource-bundle="LuaBundle" key="ui.settings.smart_close_end"/>
                    </properties>
                  </component>
                  <component id="enableGenericCheckBox" class="javax.swing.JCheckBox" binding="enableGenericCheckBox">
                    <constraints>
                      <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="2" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <text resource-bundle="LuaBundle" key="ui.settings.enable_generic"/>
                    </properties>
                  </component>
                </children>
              </grid>
              
              <!-- 填充空间 -->
              <vspacer id="basicTabSpacer">
                <constraints>
                  <grid row="5" column="1" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
                </constraints>
              </vspacer>
            </children>
          </grid>
          
          <!-- 代码完成标签页 -->
          <grid id="completionSettingsTab" layout-manager="GridLayoutManager" row-count="3" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="15">
            <margin top="15" left="15" bottom="15" right="15"/>
            <constraints>
              <tabbedpane title="代码完成"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <!-- 完成选项 -->
              <grid id="completionOptionsPanel" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="5">
                <margin top="10" left="10" bottom="10" right="10"/>
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="etched" title="完成选项"/>
                <children>
                  <component id="showWordsInFileCheckBox" class="javax.swing.JCheckBox" binding="showWordsInFile">
                    <constraints>
                      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="2" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <text resource-bundle="LuaBundle" key="ui.settings.show_words"/>
                    </properties>
                  </component>
                  <component id="recognizeGlobalNameCheckBox" class="javax.swing.JCheckBox" binding="recognizeGlobalNameAsCheckBox">
                    <constraints>
                      <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="2" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <text resource-bundle="LuaBundle" key="ui.settings.recognize_global_name_as_type"/>
                    </properties>
                  </component>
                </children>
              </grid>
              
              <!-- 类型安全 -->
              <grid id="typeSafetyPanel" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="5">
                <margin top="10" left="10" bottom="10" right="10"/>
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="etched" title-resource-bundle="LuaBundle" title-key="ui.settings.type_safety"/>
                <children>
                  <component id="enforceTypeSafetyCheckBox" class="javax.swing.JCheckBox" binding="enforceTypeSafety">
                    <constraints>
                      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="2" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <text resource-bundle="LuaBundle" key="ui.settings.enforce_type_safety"/>
                    </properties>
                  </component>
                  <component id="nilStrictCheckBox" class="javax.swing.JCheckBox" binding="nilStrict">
                    <constraints>
                      <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="2" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <text resource-bundle="LuaBundle" key="ui.settings.strict_nil_checks"/>
                    </properties>
                  </component>
                </children>
              </grid>
              
              <!-- 填充空间 -->
              <vspacer id="completionTabSpacer">
                <constraints>
                  <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
                </constraints>
              </vspacer>
            </children>
          </grid>
          
          <!-- Unreal Engine标签页 -->
          <grid id="unrealEngineTab" layout-manager="GridLayoutManager" row-count="5" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="10" vgap="10">
            <margin top="15" left="15" bottom="15" right="15"/>
            <constraints>
              <tabbedpane title="Unreal Engine"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <!-- UE项目路径 -->
              <component id="ueProjectPathLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="UE Project Path:"/>
                </properties>
              </component>
              <component id="ueProjectPathField" class="javax.swing.JTextField" binding="ueProjectPathField">
                <constraints>
                  <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                    <preferred-size width="300" height="-1"/>
                  </grid>
                </constraints>
                <properties/>
              </component>
              <grid id="ueProjectButtonsPanel" layout-manager="GridLayoutManager" row-count="1" column-count="2" same-size-horizontally="true" same-size-vertically="false" hgap="5" vgap="-1">
                <margin top="0" left="0" bottom="0" right="0"/>
                <constraints>
                  <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="browseUEProjectButton" class="javax.swing.JButton" binding="browseUEProjectButton">
                    <constraints>
                      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <text value="Browse"/>
                    </properties>
                  </component>
                  <component id="autoDetectUEProjectButton" class="javax.swing.JButton" binding="autoDetectUEProjectButton">
                    <constraints>
                      <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <text value="Auto Detect"/>
                    </properties>
                  </component>
                </children>
              </grid>
              
              <!-- UE IntelliSense选项 -->
              <component id="enableUEIntelliSenseCheckBox" class="javax.swing.JCheckBox" binding="enableUEIntelliSenseCheckBox">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="3" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="Enable UE IntelliSense"/>
                </properties>
              </component>
              
              <!-- UE进程名称 -->
               <component id="ueProcessNamesLabel" class="javax.swing.JLabel">
                 <constraints>
                   <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                 </constraints>
                 <properties>
                   <text value="UE Process Names:"/>
                 </properties>
               </component>
               <component id="ueProcessNamesField" class="javax.swing.JTextField" binding="ueProcessNamesField">
                 <constraints>
                   <grid row="2" column="1" row-span="1" col-span="2" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                     <preferred-size width="400" height="-1"/>
                   </grid>
                 </constraints>
                 <properties>
                   <toolTipText value="Enter process names separated by commas (e.g., UnrealEditor.exe, UE4Editor.exe)"/>
                 </properties>
               </component>
              
              <!-- 调试器进程黑名单 -->
               <component id="debugProcessBlacklistLabel" class="javax.swing.JLabel">
                 <constraints>
                   <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                 </constraints>
                 <properties>
                   <text value="Debug Process Blacklist:"/>
                 </properties>
               </component>
               <component id="debugProcessBlacklistField" class="javax.swing.JTextField" binding="debugProcessBlacklistField">
                 <constraints>
                   <grid row="3" column="1" row-span="1" col-span="2" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                     <preferred-size width="400" height="-1"/>
                   </grid>
                 </constraints>
                 <properties>
                   <toolTipText value="Enter process names to exclude from debug process list, separated by commas (e.g., winlogon, csrss, wininit, services)"/>
                 </properties>
               </component>
              
              <!-- 填充空间 -->
              <vspacer id="ueTabSpacer">
                <constraints>
                  <grid row="4" column="1" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
                </constraints>
              </vspacer>
            </children>
          </grid>
          
          <!-- 调试设置标签页 -->
          <grid id="debugSettingsTab" layout-manager="GridLayoutManager" row-count="4" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="10" vgap="10">
            <margin top="15" left="15" bottom="15" right="15"/>
            <constraints>
              <tabbedpane title="调试设置"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <!-- 字符集设置 -->
              <component id="charsetLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="Default charset"/>
                </properties>
              </component>
              <component id="charsetComboBox" class="javax.swing.JComboBox" binding="charsetComboBox">
                <constraints>
                  <grid row="0" column="1" row-span="1" col-span="2" vsize-policy="0" hsize-policy="2" anchor="8" fill="1" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
              </component>
              
              <!-- 自定义EmmyHelper.lua文件路径 -->
              <component id="customEmmyHelperPathLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="Custom EmmyHelper.lua Path:"/>
                </properties>
              </component>
              <component id="customEmmyHelperPathField" class="javax.swing.JTextField" binding="customEmmyHelperPathField">
                <constraints>
                  <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                    <preferred-size width="300" height="-1"/>
                  </grid>
                </constraints>
                <properties>
                  <toolTipText value="Specify a custom path to emmyHelper.lua file (leave empty to use default)"/>
                </properties>
              </component>
              <component id="browseCustomEmmyHelperButton" class="javax.swing.JButton" binding="browseCustomEmmyHelperButton">
                <constraints>
                  <grid row="1" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="Browse"/>
                </properties>
              </component>
              
              <!-- 调试选项 -->
              <grid id="debugOptionsPanel" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="5">
                <margin top="10" left="10" bottom="10" right="10"/>
                <constraints>
                  <grid row="2" column="0" row-span="1" col-span="3" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="etched" title="调试选项"/>
                <children>
                  <component id="captureStdCheckBox" class="javax.swing.JCheckBox" binding="captureStd">
                    <constraints>
                      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="2" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <selected value="true"/>
                      <text value="Capture stdout/stderr (Attach debugger)"/>
                    </properties>
                  </component>
                  <component id="captureOutputDebugStringCheckBox" class="javax.swing.JCheckBox" binding="captureOutputDebugString">
                    <constraints>
                      <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="2" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <text value="Capture OutputDebugString (Windows only)"/>
                    </properties>
                  </component>
                </children>
              </grid>
              
              <!-- 填充空间 -->
              <vspacer id="debugTabSpacer">
                <constraints>
                  <grid row="3" column="1" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
                </constraints>
              </vspacer>
            </children>
          </grid>
          
          <!-- 高级设置标签页 -->
          <grid id="advancedSettingsTab" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="15">
            <margin top="15" left="15" bottom="15" right="15"/>
            <constraints>
              <tabbedpane title="高级设置"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <!-- 附加源码根目录 -->
              <component id="additionalRootsPanel" class="com.tang.intellij.lua.project.LuaAdditionalSourcesRootPanel" binding="additionalRoots">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
              </component>
              
              <!-- 填充空间 -->
              <vspacer id="advancedTabSpacer">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
                </constraints>
              </vspacer>
            </children>
          </grid>
        </children>
      </tabbedpane>
    </children>
  </scrollpane>
</form>
